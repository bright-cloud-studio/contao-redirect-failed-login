{
    "name":"bright-cloud-studio/contao-redirect-failed-login",
    "description":"Extends the default Contao 'Login form' front end module to allow selecting of a page to forward users to after failed login attempts",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "login", "redirect", "fail", "failed"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.2",
        "contao/core-bundle": "^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/contao_redirect_failed_login/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/contao_redirect_failed_login": "system/modules/contao_redirect_failed_login"
            }
        }
    },
    "minimum-stability": "dev"
}
